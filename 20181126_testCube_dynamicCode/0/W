internalField   uniform 0; 	//set it off if using codeStream
//--------------------------------------------------------------------------------------- 

boundaryField
{
    in
    {
	//#include "codeStream/boundaryFieldW";

        type            fixedValue;	//set it off if using codeStream
        value           uniform 0;	//set it off if using codeStream
    }
    out
    {
        type            zeroGradient;
    }
    shell
    {
        type            zeroGradient;
    }
}

